<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: bisque;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@../css/style.css" fx:controller="sample.ui.memberlist.MemberlistController">
    <TableView fx:id="TableView" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="600.0" style="-fx-background-color: bisque;">
        <columns>
            <TableColumn id="col" fx:id="usernameCol" prefWidth="75.0" text="Username" />
            <TableColumn id="col1" fx:id="passwordCol" prefWidth="75.0" text="Password" />
            <TableColumn id="col1" fx:id="typeCol" prefWidth="75.0" text="Type" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
    <HBox layoutY="350.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#convert" prefHeight="40.0" prefWidth="250.0" text="Convert">
            <HBox.margin>
                <Insets left="40.0" top="5.0" />
            </HBox.margin>
            <font>
                <Font name="Comic Sans MS" size="18.0" />
            </font>
        </Button>
        <Button layoutX="50.0" layoutY="15.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#cancel" prefHeight="40.0" prefWidth="250.0" text="Cancel">
            <HBox.margin>
                <Insets top="5.0" />
            </HBox.margin>
            <font>
                <Font name="Comic Sans MS" size="18.0" />
            </font>
        </Button>
    </HBox>
</AnchorPane>
